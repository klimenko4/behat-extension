{
  "name": "samsonframework/behat-extension",
  "description": "SamsonFramework Behat generic features,steps and extenstions",
  "keywords": [
    "samsonframework",
    "behat",
    "bdd",
    "behat features",
    "behat steps"
  ],
  "authors": [
    {
      "name": "Vitaly Iegorov",
      "email": "egorov@samsonos.com"
    }
  ],
  "require": {
    "behat/behat": "@stable",
    "behat/mink-extension": "@stable",
    "behat/mink-selenium2-driver": "@stable",
    "behat/symfony2-extension": "@stable",
    "behat/mink-browserkit-driver": "@stable",
    "phpunit/phpunit": "5.*"
  },
  "license": "Open Software License (OSL) v 3.0",
  "homepage": "http://samsonos.com/",
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  },
  "config": {
    "github-protocols": [
      "https"
    ]
  },
  "autoload": {
    "psr-4": {
      "samsonframework\\behatextension\\": "src/"
    }
  }
}